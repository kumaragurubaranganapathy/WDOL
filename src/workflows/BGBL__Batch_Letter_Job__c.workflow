        <actions>
            <name>BGBL__Completed_Batch_Letter_Job_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>BGBL__Batch_Letter_Job__c.BGBL__Print_File__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>BGBL__Batch_Letter_Job__c.BGBL__Status__c</field>
            <operation>equals</operation>
            <value>Completed</value>
        </criteriaItems>
        <criteriaItems>
            <field>BGBL__Batch_Letter_Job__c.BGBL__Single_Letter_Job__c</field>

        <description>External Id</description>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MUSW__Control__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>is the number issued permanently (ie. locked and will require an extra Issued_Number__c field), or be overwritten only one more time after being set?</inlineHelpText>
        <label>Control</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Permanent</fullName>
                    <default>true</default>
                    <label>Permanent</label>
                </value>
                <value>
                    <fullName>Overwrite Once</fullName>
                    <default>false</default>
                    <label>Overwrite Once</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>MUSW__Date_Field__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>blank (today), or name of a date field on record object. Value of date field will be used when formatting date masks eg. yymmdd. Date field value must never be null</inlineHelpText>
        <label>Date Field</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MUSW__Format__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>

    <validationRules>
        <fullName>MUSW__Auto_Rule_requires_Source_Type_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(MUSW__Mode__c, &apos;Auto&apos;), ISNULL(MUSW__Source_Type_Fields__c),UPPER(Name)!=&apos;DEFAULT&apos;)</errorConditionFormula>
        <errorMessage>Auto Numbers require at least one Source Type Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MUSW__Type_not_required_if_not_in_Format_mask</fullName>
        <active>true</active>
        <description>If type mask is not included in mask format do not fill in the Type field</description>
        <errorConditionFormula>AND(NOT(CONTAINS(MUSW__Format__c,&apos;{type}&apos;)) ,  MUSW__Type__c != null)</errorConditionFormula>
        <errorMessage>Type not required if Format mask does not contain &apos;{type}&apos;. Clear Type field</errorMessage>
    </validationRules>

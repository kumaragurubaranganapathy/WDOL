<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>BGBK__Enable_DRE_Logs__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Used in DRE1; Set to TRUE during debugging to capture event logs. Note: This will cause performance slow down - it should not be on all of the time.</description>
        <externalId>false</externalId>
        <label>Rules Engine Logs Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>BGBK__Enable_DRE_Preview_Mode__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Use to debug CPU Timeout errors.</description>
        <externalId>false</externalId>
        <inlineHelpText>Use to debug CPU Timeout errors</inlineHelpText>
        <label>Rules Engine Preview Mode</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>BGBK__Exclude_Children__c</fullName>
        <deprecated>false</deprecated>
        <description>Comma-delimited list of child relationship names to be excluded from DRE DataContext. For example, MUSW__Fees__r,CustomObjects__r (notice, use &apos;__r&apos;).</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Comma-delimited list of child relationship names to be excluded from DRE DataContext. For example, MUSW__Fees__r,CustomObjects__r (notice, use &apos;__r&apos;)</inlineHelpText>
        <label>Exclude Children</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>BGBK__Parents__c</fullName>
        <deprecated>false</deprecated>
        <description>Used in DRE1 to define objects that need to trigger DRE events.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>List of comma-delimited custom objects that you can relate children records to. If no standard parent is found, objects as not-null lookup listed here first are selected as parents first.</inlineHelpText>
        <label>Additional Parents</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>BGBK__Validate_DRE_Transforms__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>If set to TRUE, the Rules Engine will validate transform values, such as repeat tags, and displays an error if there are problems. Leaving this setting unchecked causes errors to be skipped with no feedback.</description>
        <externalId>false</externalId>
        <inlineHelpText>If checked Rules Engine will validate Transform values such as repeat tags, and displays an error if there are problems. Leaving this setting unchecked causes errors to be skipped with no feedback.</inlineHelpText>
        <label>Rules Engine Validate Transforms</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>BGBK__rulesEngineActive__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Used in DRE1; Set to FALSE to disable all DRE rules.</description>
        <externalId>false</externalId>
        <label>Rules Engine Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>BGBK Global Settings</label>
    <visibility>Public</visibility>
</CustomObject>

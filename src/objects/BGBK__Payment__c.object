    <validationRules>
        <fullName>BGBK__Amount_less_than_0</fullName>
        <active>true</active>
        <errorConditionFormula>BGBK__Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Payment Amount must be positive. Cancel or correct amount to &gt;= 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Payment_Amount_less_than_Available</fullName>
        <active>true</active>
        <description>Cannot refund used amounts.</description>
        <errorConditionFormula>AND(BGBK__Amount__c - BGBK__Amount_Used__c  &lt;  BLANKVALUE(BGBK__Refunded_Amount__c,0), ISCHANGED(BGBK__Amount__c))</errorConditionFormula>
        <errorMessage>cannot adjust Payment Amount below amount Available.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Payment_Refund_greater_than_Available</fullName>
        <active>true</active>
        <description>Cannot refund used amounts.</description>
        <errorConditionFormula>AND(BGBK__Amount__c - BGBK__Amount_Used__c + BLANKVALUE(BGBK__Loss_Overage_Amount__c,0)  &lt;  BLANKVALUE(BGBK__Refunded_Amount__c,0), ISCHANGED(BGBK__Refunded_Amount__c))</errorConditionFormula>
        <errorDisplayField>BGBK__Refunded_Amount__c</errorDisplayField>
        <errorMessage>Cannot Refund more than the Available Payment Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Payment_Reject_greater_than_Available</fullName>
        <active>true</active>
        <description>Cannot reject used amounts.</description>
        <errorConditionFormula>AND(BGBK__Amount__c - BGBK__Amount_Used__c + BLANKVALUE(BGBK__Loss_Overage_Amount__c,0)  &lt;  BLANKVALUE(BGBK__Rejected_Amount__c,0), ISCHANGED(BGBK__Rejected_Amount__c))</errorConditionFormula>
        <errorDisplayField>BGBK__Rejected_Amount__c</errorDisplayField>
        <errorMessage>Cannot Reject more than the Available Payment Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Payment_Used_greater_than_Amount</fullName>
        <active>true</active>
        <description>Cannot use more than allocated</description>
        <errorConditionFormula>AND((BGBK__Amount_Used__c - BLANKVALUE(BGBK__Loss_Overage_Amount__c,0))  &gt; (BGBK__Amount__c -  BLANKVALUE(BGBK__Refunded_Amount__c,0)), ISCHANGED(BGBK__Amount_Used__c))</errorConditionFormula>
        <errorMessage>Cannot Use (pay) more than Payment Amount (minus any Refunds)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Payment_change_more_than_Receipt_avail</fullName>
        <active>true</active>
        <description>If payment amount is changed, and the parent receipt is filled in on this payment, and difference of the old amount to the new amount is greater than what is available on the receipt...error</description>
        <errorConditionFormula>AND( ISCHANGED(BGBK__Amount__c),   NOT(ISBLANK(BGBK__Receipt__c)), BGBK__Amount__c - PRIORVALUE(BGBK__Amount__c) &gt;    BGBK__Receipt__r.MUSW__Amount_Available__c)</errorConditionFormula>
        <errorMessage>Cannot increase Payment Amount greater than what is available on Receipt. Reduce Amount or cancel change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Licensee_does_not_match_the_account</fullName>
        <active>false</active>
        <description>Validation to ensure Licensee matches the account on the Deposit</description>
        <errorConditionFormula>NOT(ISBLANK( BGBK__Deposit__c)) &amp;&amp; (BGBK__Deposit__r.MUSW__Account__c  &lt;&gt;   BGBK__License2__r.MUSW__Primary_Licensee__c)</errorConditionFormula>
        <errorMessage>Licensee does not match the account on the Deposit</errorMessage>
    </validationRules>

        <fullName>MUSW__Available_Amount__c</fullName>
        <deprecated>false</deprecated>
        <description>Total remaining amount available on the deposit.</description>
        <externalId>false</externalId>
        <formula>MUSW__Paid_Amount__c - MUSW__Used_Amount__c - MUSW__Refunded_Amount__c - MUSW__Transfered_Out_Amount__c + MUSW__Transfered_In_Amount__c</formula>

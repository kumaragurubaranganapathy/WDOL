    <validationRules>
        <fullName>BGBK__Can_not_Void_a_Paid_Invoice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(BGBK__Status__c), ISPICKVAL(BGBK__Status__c, &apos;Voided&apos;), ISPICKVAL(PRIORVALUE(BGBK__Status__c), &apos;Paid&apos;))</errorConditionFormula>
        <errorMessage>Can not Void a Paid Invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBK__Can_not_change_from_Voided_Invoice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(BGBK__Status__c), ISPICKVAL(PRIORVALUE(BGBK__Status__c), &apos;Voided&apos;))</errorConditionFormula>
        <errorMessage>Can not change the Status of a Voided Invoice</errorMessage>
    </validationRules>

<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Allows activation/deactivation of DRE for objects</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DRE__Batch_Size__c</fullName>
        <defaultValue>10</defaultValue>
        <deprecated>false</deprecated>
        <description>Number of records to be processed in each batch when DRE is running in Bulk Mode</description>
        <externalId>false</externalId>
        <inlineHelpText>Number of records to be processed in each batch when DRE is running in Bulk Mode</inlineHelpText>
        <label>Bulk Batch Size</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DRE__Bulk_Threshold__c</fullName>
        <defaultValue>5</defaultValue>
        <deprecated>false</deprecated>
        <description>Maximum number of records DRE should process without kicking into Bulk Mode. Any more than this number will enable Bulk Mode (asynchronous processing). This setting is applied only if Auto-Bulk is enabled in DRE Settings</description>
        <externalId>false</externalId>
        <inlineHelpText>Maximum number of records DRE should process without kicking into Bulk Mode. Any more than this number will enable Bulk Mode (asynchronous processing). This setting is applied only if Auto-Bulk is enabled in DRE Settings</inlineHelpText>
        <label>Bulk Threshold</label>
        <precision>4</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DRE__Trigger_Enabled__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected DRE will execute when changes are made to this object</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected DRE will execute when changes are made to this object</inlineHelpText>
        <label>Trigger Enabled</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__View_Enabled__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected DRE will execute when this object is viewed (for View rules)</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected DRE will execute when this object is viewed (for View rules)</inlineHelpText>
        <label>View Enabled</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>

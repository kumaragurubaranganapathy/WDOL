        <fullName>Send_GL__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
  IF( 
    Debit_Credit__c = &quot;CR&quot;,  
    CASE(AFRS_Transaction_Code__c &amp; Reversal__c, 
      &quot;049&quot;,1,&quot;071&quot;,1,&quot;074R&quot;,1,&quot;090&quot;,1,&quot;455&quot;,1,&quot;966&quot;,1, 
      0 
    ), 
    CASE(AFRS_Transaction_Code__c &amp; Reversal__c, 
      &quot;049R&quot;,1,&quot;071R&quot;,1,&quot;074&quot;,1, &quot;951&quot;,1, &quot;955&quot;,1, &quot;965&quot;,1, 
      0 
    ) 
  )=1, 
  True, 
  False
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>

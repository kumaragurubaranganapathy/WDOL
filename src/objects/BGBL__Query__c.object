    <validationRules>
        <fullName>BGBL__Fields_Required</fullName>
        <active>true</active>
        <errorConditionFormula>BGBL__Object_Fields__c = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>BGBL__Object_Fields__c</errorDisplayField>
        <errorMessage>You must specify which fields you want the query to return for merging.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBL__Fields_id</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(CONTAINS(SUBSTITUTE(UPPER(BGBL__Object_Fields__c) , &quot; &quot;,&quot;&quot;), &quot;,ID,&quot;),BEGINS(SUBSTITUTE(UPPER(BGBL__Object_Fields__c) , &quot; &quot;,&quot;&quot;), &quot;ID,&quot;),CONTAINS(SUBSTITUTE(UPPER(BGBL__Object_Fields__c) , &quot; &quot;,&quot;&quot;), &quot;,ID&quot;)))</errorConditionFormula>
        <errorMessage>Query fields must include id field in list</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BGBL__Relationship_Queries_are_not_Supported</fullName>
        <active>true</active>
        <description>.NET letter app needs relationship queries to be defined explicitly</description>
        <errorConditionFormula>OR(CONTAINS( BGBL__Object_Fields__c, &quot;(&quot;),CONTAINS(BGBL__Object_Fields__c, &quot;)&quot;))</errorConditionFormula>
        <errorMessage>Relationship queries are not permitted with Letter Generator. Instead, create another letter query with this query as the parent to follow this one</errorMessage>
    </validationRules>

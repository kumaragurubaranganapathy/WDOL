<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configuration for GIS interface</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>MUSW__AllowCallouts__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Callouts are permitted to configured GIS REST services</description>
        <externalId>false</externalId>
        <inlineHelpText>If true, callouts are permitted to call external GIS REST services (ensure they hosted as valid Remote Sites)</inlineHelpText>
        <label>Allow Callouts</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>MUSW__GeoCodeService__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>URL for the GeoCode REST Service (include any default parameters)
eg. http://sampleserver1.arcgisonline.com/ArcGIS/rest/services/Locators/ESRI_Geocode_USA/GeocodeServer/findAddressCandidates?Address=</inlineHelpText>
        <label>DefaultGeoCodeService</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MUSW__ParcelQueryFields__c</fullName>
        <defaultValue>&quot;PARCELID&quot;</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Parcel field containing the parcel number eg. Name
or PARCELID
or Name,PARCELID</inlineHelpText>
        <label>ParcelQueryFields</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MUSW__ParcelQueryService__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>The Map Server Parcel Layer Query REST Service (incl. default params) Layer must contain Parcel Number
eg. http://host/ArcGIS/rest/services/Louisville/LOJIC_LandRecords_Louisville/MapServer/0/query?returnGeometry=true&amp;geometryType=esriGeometryPoint&amp;text=</inlineHelpText>
        <label>ParcelQueryService</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MUSW__Parent_Objects__c</fullName>
        <deprecated>false</deprecated>
        <description>Comma-separated list of object API names that can be manually created via action links in the GIS Map Viewer information popup. Current map user still needs to have correct object and lookup field permissions to be able to see Create links. Specifically creatable in their profile or equivalent permission set and lookup field is visible, and not readonly.</description>
        <externalId>false</externalId>
        <inlineHelpText>Comma-separated list of object API names that can be manually created via action links in the GIS Map Viewer information popup</inlineHelpText>
        <label>Parent Objects</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>MUSW__Routing_Service_Available__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Routing Service Available specified whether the necessary services are configured in the Directions Widget.</description>
        <externalId>false</externalId>
        <inlineHelpText>Turn this ON if Route and Locator service Urls are configured in Directions Widget of GIS Map Viewer.</inlineHelpText>
        <label>Routing Service Available</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>GIS Settings</label>
    <visibility>Public</visibility>
</CustomObject>

    <fields>
        <fullName>Biennium_year__c</fullName>
        <externalId>false</externalId>
        <formula>IF(MOD(Year(Document_Date__c),2) = 1,
	IF(Document_Date__c &gt; DATE(year(Document_Date__c),06,30), Text(Year(Document_Date__c +2)), 	 
        Text(Year(Document_Date__c))),	Text(Year(Document_Date__c +1))
	)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Biennium Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Document_Date__c</fullName>
        <externalId>false</externalId>
        <label>Document Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Fiscal_Month__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( 
Month(Document_Date__c), 
1, 07, 
2, 20, 
3, 21, 
4, 22, 
5, 23, 
6, 24, 
7, 01, 
8, 02, 
9, 03, 
10, 04, 
11, 05, 
06)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Fiscal Month</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>

<aura:component controller = "Lnp_CommonHeader_AC" implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="customLblMap" type="Map" />
    <aura:attribute name="currentYear" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
        
    <div class="footerWrapper" >  		       
        <div class="slds-grid botNav">
            <ul class="footerList">
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_FooterTerms_URL}" target='blank'>{!v.customLblMap.LnP_Footer_Terms}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_FooterPrivacy_URL}" target='blank'>{!v.customLblMap.LnP_Footer_Privacy}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_FooterGuide_URL}" target='blank'>{!v.customLblMap.LnP_Footer_Guide}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_FooterSupport_URL}" target='blank'>{!v.customLblMap.LnP_Footer_Support}</a></li>
                <li class="footerListItem"><span class="footerListLink copyright">{!'Copyright Â© ' + v.currentYear}<img src="{!$Resource.LnP_Icons+'/LnP_Icons/lock.png'}" alt="lock" /></span></li>
            </ul>                	
        </div>        
    </div>
</aura:component>
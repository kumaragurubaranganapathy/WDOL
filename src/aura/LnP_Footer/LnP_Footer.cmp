<aura:component controller = "Lnp_CommonHeader_AC" implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="customLblMap" type="Map" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
        
    <div class="footerWrapper" >  		       
        <div class="slds-grid botNav">
            <ul class="footerList">
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_Footer}" target='blank'>{!v.customLblMap.LnP_Footer_Terms}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_Footer}" target='blank'>{!v.customLblMap.LnP_Footer_Privacy}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_Footer}" target='blank'>{!v.customLblMap.LnP_Footer_Guide}</a></li>
                <li class="footerListItem"><a class="footerListLink" href="{!v.customLblMap.LnP_Footer}" target='blank'>{!v.customLblMap.LnP_Footer_Support}</a></li>
            </ul>                	
        </div>        
    </div>
</aura:component>
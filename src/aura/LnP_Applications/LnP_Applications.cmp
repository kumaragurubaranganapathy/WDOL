<aura:component implements="forceCommunity:availableForAllPageTypes" access="global" controller="LnP_ApplicationsController">
    APPLICATIONS
    <aura:attribute name="applications" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div>
        <aura:iteration items="{!v.applications}" var="app">
            <div><div class="slds-col">
            <a onclick="{!c.goToApplication}" id="{!app.Id}">{!app.Id}</a>
            </div>
</div>
            
        </aura:iteration>
   </div>
</aura:component>
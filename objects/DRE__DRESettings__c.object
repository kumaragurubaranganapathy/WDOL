<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Controls behaviour of DRE</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DRE__IsAutoBulk__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected DRE will automatically process records in separate batches. Doing so can increase system robustness at the cost of losing real-time feedback. Enable this setting if your organization contains complex automation processes</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected DRE will automatically process records in separate batches. Doing so can increase system robustness at the cost of losing real-time feedback. Enable this setting if your organization contains complex automation processes</inlineHelpText>
        <label>Enable Auto-Bulk</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__IsBatchLogging__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected creates log records for DRE batch rules in the DRE Event Log table</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected creates log records for DRE batch rules in the DRE Event Log table</inlineHelpText>
        <label>Enable Batch Logs</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__IsDebugMode__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected DRE will display more detailed error messages (not user friendly)</description>
        <externalId>false</externalId>
        <label>Enable Debug Mode</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__IsEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Enable DRE</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__IsLogging__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected creates log records for DRE events in the DRE Event Log table</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected creates log records for DRE events in the DRE Event Log table</inlineHelpText>
        <label>Enable Event Logs</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DRE__Suppress_Errors__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected all DRE errors will be suppressed and logged in the DRE Event Log table, thereby preventing users from receiving real-time feedback when error occurs</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected all DRE errors will be suppressed and logged in the DRE Event Log table, thereby preventing users from receiving real-time feedback when error occurs</inlineHelpText>
        <label>Suppress Errors</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>DRE Settings</label>
    <visibility>Public</visibility>
</CustomObject>
